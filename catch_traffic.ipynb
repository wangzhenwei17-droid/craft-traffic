    "def analyze_dns_queries(packet):\n",
    "    \"\"\"ÂàÜÊûêDNSÊü•ËØ¢\"\"\"\n",
    "    if packet.haslayer(scapy.DNSQR):  # DNSÊü•ËØ¢ËÆ∞ÂΩï\n",
    "        dns = packet[scapy.DNSQR]\n",
    "        \n",
    "        print(\"üîç DNSÊü•ËØ¢:\")\n",
    "        print(f\"   Êü•ËØ¢ÂüüÂêç: {dns.qname.decode('utf-8', errors='ignore')}\")\n",
    "        print(f\"   Êü•ËØ¢Á±ªÂûã: {dns.qtype}\")\n",
    "        \n",
    "        if packet.haslayer(scapy.DNSRR):  # DNSÂìçÂ∫îËÆ∞ÂΩï\n",
    "            for i in range(packet[scapy.DNS].ancount):\n",
    "                answer = packet[scapy.DNSRR][i]\n",
    "                print(f\"   ÂìçÂ∫î {i+1}: {answer.rdata} ({answer.type})\")\n",
    "        \n",
    "        print(\"-\" * 30)\n",
    "        return True\n",
    "    return False"
   ]
