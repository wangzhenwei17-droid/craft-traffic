    "def advanced_sniffer(timeout=30):\n",
    "    \"\"\"È´òÁ∫ßÊµÅÈáèÂàÜÊûêÂô®\"\"\"\n",
    "    print(f\"üî¨ ÂºÄÂßãÈ´òÁ∫ßÊµÅÈáèÂàÜÊûêÔºà{timeout}ÁßíÔºâ...\")\n",
    "    print(\"ÂàÜÊûêÂÜÖÂÆπÔºö\")\n",
    "    print(\"  1. HTTPËØ∑Ê±Ç\")\n",
    "    print(\"  2. DNSÊü•ËØ¢\")\n",
    "    print(\"  3. TCPËøûÊé•\")\n",
    "    print(\"-\" * 50)\n",
    "    \n",
    "    stats = {\n",
    "        'total': 0,\n",
    "        'http': 0,\n",
    "        'dns': 0,\n",
    "        'tcp': 0,\n",
    "        'udp': 0\n",
    "    }\n",
    "    \n",
    "    def advanced_callback(packet):\n",
    "        stats['total'] += 1\n",
    "        \n",
    "        # HTTPÂàÜÊûê\n",
    "        if analyze_http_traffic(packet):\n",
    "            stats['http'] += 1\n",
    "        \n",
    "        # DNSÂàÜÊûê\n",
    "        if analyze_dns_queries(packet):\n",
    "            stats['dns'] += 1\n",
    "        \n",
    "        # ÂçèËÆÆÁªüËÆ°\n",
    "        if packet.haslayer(scapy.TCP):\n",
    "            stats['tcp'] += 1\n",
    "        elif packet.haslayer(scapy.UDP):\n",
    "            stats['udp'] += 1\n",
    "        \n",
    "        # ÊØè10‰∏™ÂåÖÊòæÁ§∫‰∏ÄÊ¨°ËøõÂ∫¶\n",
    "        if stats['total'] % 10 == 0:\n",
    "            print(f\"üìä Â∑≤ÂàÜÊûê {stats['total']} ‰∏™ÂåÖ...\")\n",
    "    \n",
    "    # ÂºÄÂßãÊçïËé∑\n",
    "    scapy.sniff(prn=advanced_callback, store=0, timeout=timeout)\n",
    "    \n",
    "    # ÊòæÁ§∫ÁªüËÆ°ÁªìÊûú\n",
    "    print(\"=\" * 50)\n",
    "    print(\"üìà ÊµÅÈáèÂàÜÊûêÁªüËÆ°:\")\n",
    "    print(f\"   ÊÄªÊï∞ÊçÆÂåÖ: {stats['total']}\")\n",
    "    print(f\"   HTTPÊµÅÈáè: {stats['http']}\")\n",
