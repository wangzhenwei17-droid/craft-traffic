    "def simple_packet_sniffer(count=10):\n",
    "    \"\"\"ÊçïËé∑ÊåáÂÆöÊï∞ÈáèÁöÑÊï∞ÊçÆÂåÖ\"\"\"\n",
    "    print(f\"üéØ ÂºÄÂßãÊçïËé∑ {count} ‰∏™Êï∞ÊçÆÂåÖ...\")\n",
    "    print(\"Êåâ Ctrl+C ÂÅúÊ≠¢ÊçïËé∑\")\n",
    "    print(\"-\" * 50)\n",
    "    \n",
    "    packets = []\n",
    "    \n",
    "    def packet_callback(packet):\n",
    "        \"\"\"Êï∞ÊçÆÂåÖÂõûË∞ÉÂáΩÊï∞\"\"\"\n",
    "        if packet.haslayer(scapy.IP):\n",
    "            src_ip = packet[scapy.IP].src\n",
    "            dst_ip = packet[scapy.IP].dst\n",
    "            protocol = packet[scapy.IP].proto\n",
    "            \n",
    "            proto_name = {6: \"TCP\", 17: \"UDP\", 1: \"ICMP\"}.get(protocol, str(protocol))\n",
    "            \n",
    "            print(f\"üì¶ ÂåÖ #{len(packets)+1}: {src_ip:15} ‚Üí {dst_ip:15} | {proto_name}\")\n",
    "            \n",
    "            packets.append(packet)\n",
    "            \n",
    "            # ÊçïËé∑Âà∞ÊåáÂÆöÊï∞ÈáèÂêéÂÅúÊ≠¢\n",
    "            if len(packets) >= count:\n",
    "                return True  # ËøîÂõûTrueÂÅúÊ≠¢ÊçïËé∑\n",
    "    \n",
    "    # ÂºÄÂßãÊçïËé∑\n",
    "    scapy.sniff(prn=packet_callback, store=0, count=count)\n",
    "    \n",
    "    print(\"-\" * 50)\n",
    "    print(f\"‚úÖ ÊçïËé∑ÂÆåÊàêÔºÅÂÖ±ÊçïËé∑ {len(packets)} ‰∏™Êï∞ÊçÆÂåÖ\")\n",
    "    return packets\n"
