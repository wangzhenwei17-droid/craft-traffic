    "def analyze_http_traffic(packet):\n",
    "    \"\"\"ÂàÜÊûêHTTPÊµÅÈáè\"\"\"\n",
    "    if packet.haslayer(scapy.TCP) and packet.haslayer(scapy.Raw):\n",
    "        try:\n",
    "            # Ëé∑ÂèñÂéüÂßãÊï∞ÊçÆ\n",
    "            raw_data = packet[scapy.Raw].load.decode('utf-8', errors='ignore')\n",
    "            \n",
    "            # Ê£ÄÊü•ÊòØÂê¶‰∏∫HTTPËØ∑Ê±Ç\n",
    "            if \"HTTP\" in raw_data[:100]:\n",
    "                print(\"üåê ÂèëÁé∞HTTPÊµÅÈáè:\")\n",
    "                print(\"-\" * 40)\n",
    "                \n",
    "                # ÊèêÂèñHTTPÊñπÊ≥ï\n",
    "                lines = raw_data.split('\\r\\n')\n",
    "                request_line = lines[0] if lines else \"\"\n",
    "                \n",
    "                if request_line:\n",
    "                    print(f\"ËØ∑Ê±Ç: {request_line}\")\n",
    "                \n",
    "                # ÊèêÂèñHostÂ§¥ÈÉ®\n",
    "                for line in lines[1:]:\n",
    "                    if line.lower().startswith('host:'):\n",
    "                        print(f\"‰∏ªÊú∫: {line[5:].strip()}\")\n",
    "                    elif line.lower().startswith('user-agent:'):\n",
    "                        print(f\"Áî®Êà∑‰ª£ÁêÜ: {line[11:].strip()[:50]}...\")\n",
    "                \n",
    "                print(\"-\" * 40)\n",
    "                return True\n",
    "        except:\n",
    "            pass\n",
    "    return False\n"
   ]
